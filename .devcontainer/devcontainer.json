{
  "name": "commitlint-dev",
  "dockerComposeFile": ["../docker-compose.yml"],
  "service": "commitlint",
  "workspaceFolder": "/root/repo",
  "shutdownAction": "stopCompose",
  "customizations": {
    "vscode": {
      "extensions": [
        "editorconfig.editorconfig",
        "esbenp.prettier-vscode",
        "ms-azuretools.vscode-docker",
        "ms-vsliveshare.vsliveshare-pack"
      ]
    }
  }
}
